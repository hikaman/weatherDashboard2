{
	"name": "weatherwise-dashboard",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "bun run vite dev",
		"build": "bun run vite build",
		"preview": "bun run vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "bun run vitest run",
		"test:watch": "bun run vitest",
		"test:coverage": "bun run vitest run --coverage",
		"lint": "bun run eslint .",
		"lint:fix": "bun run eslint . --fix",
		"format": "bun run prettier --write .",
		"format:check": "bun run prettier --check ."
	},
	"dependencies": {
		"@sveltejs/kit": "^2.16.0",
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/typography": "^0.5.16",
		"@testing-library/svelte": "^5.2.3",
		"@testing-library/jest-dom": "^6.6.3",
		"@typescript-eslint/eslint-plugin": "^8.18.2",
		"@typescript-eslint/parser": "^8.18.2",
		"@vitest/coverage-v8": "^2.1.8",
		"autoprefixer": "^10.4.21",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.46.0",
		"jsdom": "^25.0.1",
		"postcss": "^8.5.3",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.2",
		"svelte-check": "^4.0.0",
		"tailwindcss": "^3.4.0",
		"typescript": "^5.0.0",
		"vite": "^6.2.6",
		"vitest": "^2.1.8"
	}
}
